<%= form_for :game do |f| %> 
<div>
    <%= f.label :game, class: "label"%>
    <%= f.text_field :gamename, class: "input"%>
  </div>
    <p>
     <%= f.label :genre, class: "label"%>
   <%= f.collection_check_boxes(:genre_id, Genre.all, :id, :thegenres) %>   
  </p> 

  
   <p>
        <%= f.submit %>
    </p>    
    
    <% end %>
